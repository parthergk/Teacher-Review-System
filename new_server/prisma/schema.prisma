generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CollegeId {
  id Int @unique @default(autoincrement())
  collegeId String @unique
}

model Student {
  id Int @unique @default(autoincrement())
  collegeId String @unique
  password String
}

model Review {
  id Int @unique
  collegeName String
  departmentName String
  teacherName String
  review String
  rating Int
}